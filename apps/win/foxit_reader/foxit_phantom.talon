os: windows
and title: /- Foxit Phantom/
-

go back[ward]:
    key("alt-left")

go for[ward]:
    key("alt-right")

on:
    key("pagedown")    

up:
    key("pageup")    

zoom out:
    key("ctrl--")

zoom in:
    key("ctrl-=")

search:
    key(ctrl-f)
    
netch:
    key(f3) 

pencil tool:
    key("alt-ctrl-p") 

eraser tool:
    key("alt-ctrl-e") 

hand tool:
    key("alt-ctrl-h") 